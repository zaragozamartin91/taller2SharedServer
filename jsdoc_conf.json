{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "model/",
            "controllers/",
            "config/",
            "middleware/",
            "routes/"
        ],
        "exclude": [
            "node_modules/"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}